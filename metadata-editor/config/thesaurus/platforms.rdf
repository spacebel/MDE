  <rdf:Description rdf:about="https://earth.esa.int/concept/oceansat-2">
    <skos:prefLabel xml:lang="en">OceanSat-2</skos:prefLabel>
    <skos:altLabel xml:lang="en">IRS-O2</skos:altLabel>
    <skos:definition xml:lang="en">OceanSat-2 is an Indian satellite designed to provide service continuity for operational users of the Ocean Colour Monitor (OCM) instrument on OceanSat-1. The main objectives of OceanSat-2 are to study surface winds and ocean surface strata, observation of chlorophyll concentrations, monitoring of phytoplankton blooms, study of atmospheric aerosols and suspended sediments in the water.</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:exactMatch rdf:resource="https://gcmdservices.gsfc.nasa.gov/kms/concept/df967339-0096-4445-8732-0071f1de9e27"/>
    <rdfs:seeAlso rdf:resource="https://directory.eoportal.org/web/eoportal/satellite-missions/o/oceansat-2"/>
    <skos:broader rdf:resource="https://earth.esa.int/concept/oceansat"/>
    <skos:note xml:lang="en">2</skos:note>
    <skos:inScheme rdf:resource="https://earth.esa.int/concepts/concept_scheme/platforms"/>
    <skos:notation xml:lang="en">OC2</skos:notation>
    <rdfs:label xml:lang="en">OceanSat-2</rdfs:label>
    <rdfs:seeAlso rdf:resource="https://earth.esa.int/web/guest/missions/3rd-party-missions/current-missions/oceansat-2"/>
  </rdf:Description>
