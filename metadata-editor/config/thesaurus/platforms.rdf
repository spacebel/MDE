    <skos:altLabel xml:lang="en">SCISAT-1/ACE</skos:altLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:seeAlso rdf:resource="https://earth.esa.int/web/guest/missions/3rd-party-missions/current-missions/scisat"/>
    <skos:exactMatch rdf:resource="https://gcmdservices.gsfc.nasa.gov/kms/concept/5419ac51-33aa-4f66-bc37-9f2c73846c9e"/>
