    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:exactMatch rdf:resource="https://gcmdservices.gsfc.nasa.gov/kms/concept/8781da14-5ced-4d64-81cd-8daa10a1c30d"/>
