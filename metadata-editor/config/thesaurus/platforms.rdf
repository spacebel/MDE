  <rdf:Description rdf:about="https://earth.esa.int/concept/dmc-2">
    <skos:exactMatch rdf:resource="https://gcmdservices.gsfc.nasa.gov/kms/concept/e5184d15-eec8-4703-8318-243748ddbd0e"/>
    <skos:narrower rdf:resource="https://earth.esa.int/concept/nigeriasat-x"/>
    <skos:narrower rdf:resource="https://earth.esa.int/concept/deimos-1"/>
    <skos:note xml:lang="en">2</skos:note>
    <skos:narrower rdf:resource="https://earth.esa.int/concept/nigeriasat-2"/>
    <skos:narrower rdf:resource="https://earth.esa.int/concept/uk-dmc-2"/>
    <skos:notation xml:lang="en">DM2</skos:notation>
    <skos:inScheme rdf:resource="https://earth.esa.int/concepts/concept_scheme/platforms"/>
    <skos:prefLabel xml:lang="en">DMC Second Generation</skos:prefLabel>
    <skos:definition xml:lang="en">The Disaster Monitoring Constellation (DMC) is an international program initially proposed in 1996 and led by SSTL (Surrey Satellite Technology Ltd), Surrey, UK, to construct a network of five affordable Low Earth Orbit (LEO) microsatellites. The objective is to provide a daily global imaging capability at medium resolution (30-40 m), in 3-4 spectral bands, for rapid-response disaster monitoring and mitigation.</skos:definition>
    <skos:broader rdf:resource="https://earth.esa.int/concept/dmc"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:altLabel xml:lang="en">DMC-2G (Disaster Monitoring Constellation- 2nd Generation)</skos:altLabel>
    <rdfs:seeAlso rdf:resource="https://earth.esa.int/web/guest/missions/3rd-party-missions/current-missions/uk-dmc"/>
    <rdfs:label xml:lang="en">DMC constellation Second Generation</rdfs:label>
  </rdf:Description>
