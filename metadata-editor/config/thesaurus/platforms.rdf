  <rdf:Description rdf:about="https://earth.esa.int/concept/ers">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:broader rdf:resource="https://earth.esa.int/concept/earth-observation-satellite"/>
    <skos:notation xml:lang="en">ER1,ER2</skos:notation>
    <skos:exactMatch rdf:resource="https://gcmdservices.gsfc.nasa.gov/kms/concept/3b6b4870-ae80-4488-b9fb-f9152037ec59"/>
    <rdfs:label xml:lang="en">ERS missions</rdfs:label>
    <skos:altLabel xml:lang="en">ERS Earth Resource Satellite</skos:altLabel>
    <skos:definition xml:lang="en">ESA's two European Remote Sensing (ERS) satellites, ERS-1 and –2, were launched into the same orbit in 1991 and 1995 respectively. Their payloads included a synthetic aperture imaging radar, radar altimeter and instruments to measure ocean surface temperature and wind fields. ERS-2 added an additional sensor for atmospheric ozone monitoring. The two satellites acquired a combined data set extending over two decades.</skos:definition>
    <skos:note xml:lang="en">1.2</skos:note>
    <skos:prefLabel xml:lang="en">ERS</skos:prefLabel>
    <skos:inScheme rdf:resource="https://earth.esa.int/concepts/concept_scheme/platforms"/>
    <rdfs:seeAlso rdf:resource="https://earth.esa.int/web/guest/missions/esa-operational-eo-missions/ers"/>
    <skos:narrower rdf:resource="https://earth.esa.int/concept/ers-1"/>
    <skos:narrower rdf:resource="https://earth.esa.int/concept/ers-2"/>
  </rdf:Description>
