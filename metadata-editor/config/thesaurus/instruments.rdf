    <sosa:isHostedBy rdf:resource="https://earth.esa.int/concept/swarm-c"/>
    <skos:altLabel xml:lang="en">SWARM-LRR</skos:altLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
