  <rdf:Description rdf:about="https://earth.esa.int/concept/irs-1c">
    <skos:prefLabel xml:lang="en">IRS-1C</skos:prefLabel>
    <skos:inScheme rdf:resource="https://earth.esa.int/concepts/concept_scheme/platforms"/>
    <skos:note xml:lang="en">1C</skos:note>
    <skos:exactMatch rdf:resource="https://gcmdservices.gsfc.nasa.gov/kms/concept/0b60f93d-dad7-4bb8-a41b-22d5f5d58835"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">The Indian Remote Sensing Satellite IRS-1C was successfully launched into polar orbit on 28 December 1995 by a Russian launch vehicle. Its sensors were activated in the first week of January 1996.</skos:definition>
    <rdfs:seeAlso rdf:resource="https://earth.esa.int/web/guest/missions/3rd-party-missions/current-missions/irs-1c"/>
    <rdfs:label xml:lang="en">IRS-1C</rdfs:label>
    <skos:broader rdf:resource="https://earth.esa.int/concept/irs"/>
    <skos:notation xml:lang="en">I1C</skos:notation>
