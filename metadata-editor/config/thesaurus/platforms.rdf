    <skos:exactMatch rdf:resource="https://gcmdservices.gsfc.nasa.gov/kms/concept/f3230e87-898e-45d1-aa7f-b5b42eb3a3fc"/>
    <rdfs:label xml:lang="en">JERS-1</rdfs:label>
