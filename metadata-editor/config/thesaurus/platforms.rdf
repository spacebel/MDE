  <rdf:Description rdf:about="https://earth.esa.int/concept/proba-v">
    <skos:note xml:lang="en">V</skos:note>
    <skos:altLabel xml:lang="en">Proba-V</skos:altLabel>
    <rdfs:seeAlso rdf:resource="https://directory.eoportal.org/web/eoportal/satellite-missions/p/proba-v"/>
    <skos:broader rdf:resource="https://earth.esa.int/concept/proba"/>
