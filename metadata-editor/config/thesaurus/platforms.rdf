    <skos:exactMatch rdf:resource="https://earth.esa.int/concept/jpss"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://earth.esa.int/concept/dmc">
    <skos:definition xml:lang="en">The Disaster Monitoring Constellation (DMC) is an international program initially proposed in 1996 and led by SSTL (Surrey Satellite Technology Ltd), Surrey, UK, to construct a network of five affordable Low Earth Orbit (LEO) microsatellites. The objective is to provide a daily global imaging capability at medium resolution (30-40 m), in 3-4 spectral bands, for rapid-response disaster monitoring and mitigation.</skos:definition>
    <skos:note xml:lang="en">1.2</skos:note>
    <skos:notation xml:lang="en">DM1,DM2</skos:notation>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:broader rdf:resource="https://earth.esa.int/concept/earth-observation-satellite"/>
    <skos:narrower rdf:resource="https://earth.esa.int/concept/dmc-1"/>
    <rdfs:label xml:lang="en">DMC constellation</rdfs:label>
    <skos:inScheme rdf:resource="https://earth.esa.int/concepts/concept_scheme/platforms"/>
    <skos:prefLabel xml:lang="en">DMC</skos:prefLabel>
    <skos:narrower rdf:resource="https://earth.esa.int/concept/dmc-2"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://earth.esa.int/concept/gpm">
    <rdfs:label xml:lang="en">GPM</rdfs:label>
    <skos:prefLabel xml:lang="en">GPM</skos:prefLabel>
    <skos:exactMatch rdf:resource="https://gcmdservices.gsfc.nasa.gov/kms/concept/33a893cb-b328-462e-9cb0-d8c27823239e"/>
    <skos:definition xml:lang="en">GPM Constellation is a joint mission with the Japan Aerospace Exploration Agency (JAXA) and other international partners. Building upon the success of the Tropical Rainfall Measuring Mission (TRMM), it will initiate the measurement of global precipitation, a key climate factor.</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
