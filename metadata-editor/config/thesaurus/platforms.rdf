  <rdf:Description rdf:about="https://earth.esa.int/concept/worldview-1">
    <skos:exactMatch rdf:resource="https://gcmdservices.gsfc.nasa.gov/kms/concept/7f13b4d2-9114-4890-ac6d-30da1a333d74"/>
    <rdfs:seeAlso rdf:resource="https://earth.esa.int/eogateway/search?text=worldview-1"/>
