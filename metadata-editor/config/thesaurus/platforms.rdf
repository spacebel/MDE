    <skos:prefLabel xml:lang="en">IKONOS-2</skos:prefLabel>
    <rdfs:label xml:lang="en">IKONOS-2</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
